{
    "C_Cpp.default.includePath": [
        "${env:GDK}\\inc",
        "${env:GDK}\\res",
        "res",
        "inc"
    ],
    "clangd.arguments": [
        "--header-insertion=never",
        "--function-arg-placeholders=0",
    ],
    "clangd.fallbackFlags": [
        "-I${env:GDK}\\res",
        "-I${env:GDK}\\inc",
        "-I${workspaceRoot}\\res",
        "-I${workspaceRoot}\\inc"
    ],
    "files.associations": {
        "*.h": "c",
        "*.tsx": "typescriptreact",
        "differential-update-comments": "git-commit",
        "new-commit": "git-commit"
    }
}
